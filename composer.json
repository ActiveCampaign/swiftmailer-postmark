{
    "name": "wildbit/swiftmailer-postmark",
    "description": "A Swiftmailer Transport for Postmark.",
    "require": {
        "swiftmailer/swiftmailer": "~5.1",
        "guzzlehttp/guzzle": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "illuminate/mail": "~5.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-0": {
            "Postmark\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "src/",
            "tests/"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "authors": [{
        "name": "Postmark",
        "email": "support@postmarkapp.com"
    }]
}
